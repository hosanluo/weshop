{
    "name": "michael/weshop",
    "description": "A package for wechat's shop component",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WechatShop\\": "src/",
            "WechatShop\\Tests\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WeShop\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Michael",
            "email": "hosanluo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-fileinfo": "*",
        "ext-openssl": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*",
        "ext-curl": "*",
        "monolog/monolog": "^2.2",
        "nyholm/psr7": "^1.5",
        "nyholm/psr7-server": "^1.0",
        "psr/simple-cache": "^1.0|^2.0|^3.0",
        "psr/http-client": "^1.0",
        "symfony/cache": "^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/psr-http-message-bridge": "^2.1.2",
        "symfony/http-client": "^5.4|^6.0",
        "symfony/mime": "^5.4|^6.0",
        "symfony/polyfill-php81": "^1.25",
        "overtrue/wechat": "^4.0|^5.0|^6.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.5.0",
        "brainmaestro/composer-git-hooks": "^2.8",
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1.4.4",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.2",
        "jetbrains/phpstorm-attributes": "^1.0"
    },
    "config": {
        "allow-plugins": {
            "easywechat-composer/easywechat-composer": true
        }
    }
}
